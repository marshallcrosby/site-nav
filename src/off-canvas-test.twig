	<script id="example1">

		// Initialize when the DOM is loaded
		document.addEventListener('DOMContentLoaded', () => {
			
			// Initialize navbar
			multilevelNav('.example-navbar', {
				hoverIntent: true,
				hoverIntentTimeout: 300,
				wholeLinkToggler: true,
				toggleOnClickOnly: true,
				excludeLevel: 2
			});
			
			multilevelNavSlideOver('.example-navbar', {
				slideTitleLink: true,
				offCanvasCloseAllMenus: true
			});
		});
